ter_list {\n  box-sizing: border-box;\n  margin: 10px 0px;\n  width: 70%;\n  padding: 20px 15px 30px;\n  .isActive {\n    color: #3a85ee;\n  }\n  .chapter {\n    margin: 10px 0px;\n    margin-bottom: 20px;\n    max-height: 500px;\n    overflow: auto;\n  }\n  .chapter_item {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    padding: 5px;\n    cursor: pointer;\n    margin: 10px;\n\n    &:hover {\n      background: #e9e9e9;\n    }\n    .chapter_time {\n      display: flex;\n      align-items: center;\n      color: #666;\n      box-sizing: border-box;\n      img {\n        padding-right: 5px;\n      }\n    }\n  }\n}\n\n.worklist {\n  .work_box {\n    max-height: 180px;\n    overflow: auto;\n    .work_item {\n      color: #333;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      margin: 5px 2px;\n      padding: 3px;\n      i {\n        font-weight: bold;\n      }\n      &:hover {\n        background: #e9e9e9;\n        i {\n          color: #3a85ee;\n          cursor: pointer;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"mappings":";;;;;AAwFA,OAAAA,WAAA;AACA,OAAAC,GAAA;AACA,SAAAC,qBAAA;AACA,OAAAC,UAAA;AACA,SAAAC,UAAA;AACA;AACA,OAAAC,SAAA;AACA,OAAAC,GAAA;AACA,OAAAC,sBAAA;AACA;EACAC,UAAA;IACAR,WAAA,EAAAA,WAAA;IACAG,UAAA,EAAAA;EACA;EACAM,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,YAAA;MACAC,YAAA;MACAC,UAAA;MACAC,SAAA;MACAC,QAAA;MACAC,UAAA;MACAZ,UAAA,EAAAA,UAAA;MACAa,IAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,cAAA;IACAC,OAAA,CAAAC,GAAA;EACA;EAEAC,OAAA;IACAC,SAAA,WAAAA,UAAAC,GAAA;MAAA,IAAAC,KAAA;MAAA,IAAAC,IAAA,GAAAC,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA;MAAA,IAAAG,KAAA,GAAAH,SAAA,CAAAC,MAAA,OAAAD,SAAA,MAAAE,SAAA;MAAA,IAAArB,IAAA,GAAAmB,SAAA,CAAAC,MAAA,OAAAD,SAAA,MAAAE,SAAA;MACA,KAAAZ,MAAA,OAAAb,SAAA;QACA2B,SAAA;QACAC,KAAA,EAAAxB,IAAA;QACAyB,GAAA,EAAAT,GAAA;QACAE,IAAA,EAAAA,IAAA;QACAQ,QAAA,EAAAR,IAAA;QACAS,IAAA;QACAC,MAAA;QACAC,MAAA,EAAAP,KAAA;QACAQ,KAAA;QACAC,MAAA;QACA