ializeRoutes();\n  },\n  methods: {\n    initializeRoutes() {\n      // 手动添加的路由\n      this.routers = [\n        {\n          path: 'operationSquare',\n          name: 'operationSquare',\n          meta: {\n            title: \"广场\",\n            // icon: \"user\"\n          },\n          component: operationSquare\n        },\n        {\n          path: 'myWork', // 待定的路径\n          name: 'myWork',\n          meta: {\n            title: \"我的作业\",\n\n          },\n          component: myWork\n        }\n      ];\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.user-nav-menu {\n  height: 100%;\n  .el-menu-vertical {\n    height: 100%;\n    border-right: none;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n  }\n}\n</style>\n"],"mappings":"AAiBA,OAAAA,eAAA;AACA,OAAAC,MAAA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACAC,YAAA,WAAAA,aAAA;MACA,YAAAC,MAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACAD,gBAAA,WAAAA,iBAAA;MACA;MACA,KAAAN,OAAA,IACA;QACAI,IAAA;QACAI,IAAA;QACAC,IAAA;UACAC,KAAA;UACA;QACA;QACAC,SAAA,EAAAd;MACA,GACA;QACAO,IAAA;QAAA;QACAI,IAAA;QACAC,IAAA;UACAC,KAAA;QAEA;QACAC,SAAA,EAAAb;MACA,EACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}